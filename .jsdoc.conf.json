{
    "plugins": [],
    "recurseDepth": 100,
    "opts": {
        "recurse": true,
        "destination": "./docs/",
        "template": "node_modules/clean-jsdoc-theme",
        "theme_opts": {
            "theme": "dark"
        }
    },
    "source": {
        "include": ["src"],
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "node_modules"
    },
    "sourceType": "module",
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc", "closure"]
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    }
}
